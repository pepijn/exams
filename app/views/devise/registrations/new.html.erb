<h2>Aanmelden</h2>

<%= simple_form_for resource, :as => resource_name, :url => user_registration_path do |f| %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.input :student_number %>
  <%= f.input :coupon %>
  <%= f.submit "Aanmelden" %>
<% end %>

